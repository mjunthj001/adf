{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azureadfpocdemo2"
		},
		"LSPRB_servicePrincipalCredential": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalCredential' of 'LSPRB'"
		},
		"AZsqlDes_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sqlconnection"
		},
		"AzuresqlKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ADFKeyVaul.vault.azure.net/"
		},
		"LSPRB_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "32fc61d6-3e9c-4f2c-a78b-8a1531bc3198                "
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azureadfpocdemo2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LSPRB_servicePrincipalCredential": {
						"value": "[parameters('LSPRB_servicePrincipalCredential')]"
					},
					"AzuresqlKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzuresqlKeyVault_properties_typeProperties_baseUrl')]"
					},
					"LSPRB_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('LSPRB_properties_typeProperties_servicePrincipalId')]"
					},
					"AZsqlDes_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AZsqlDes_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}