{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azureadfpocdemo2"
		},
		"lS_Scr_crm_servicePrincipalCredential": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalCredential' of 'lS_Scr_crm'"
		},
		"LS_AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSqlDatabase1'"
		},
		"Dataverse_to_SQL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Dataverse_to_SQL'"
		},
		"lS_Scr_crm_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "3e41e52c-8af6-4844-be81-31a2dcd0c887"
		},
		"Dataverse_to_SQL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfpoc1234567.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azureadfpocdemo2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lS_Scr_crm_servicePrincipalCredential": {
						"value": "[parameters('lS_Scr_crm_servicePrincipalCredential')]"
					},
					"LS_AzureSqlDatabase1_connectionString": {
						"value": "[parameters('LS_AzureSqlDatabase1_connectionString')]"
					},
					"Dataverse_to_SQL_accountKey": {
						"value": "[parameters('Dataverse_to_SQL_accountKey')]"
					},
					"lS_Scr_crm_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('lS_Scr_crm_properties_typeProperties_servicePrincipalId')]"
					},
					"Dataverse_to_SQL_properties_typeProperties_url": {
						"value": "[parameters('Dataverse_to_SQL_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}